@mixin border-radius($angle) {
  -webkit-border-radius: $angle;
  border-radius: $angle;
}

.custom-top-menu-container {
  width: 830px;

  .navbar-inner {
      background: #ffffff;
  }
}

.navbar .nav > li > a {
    padding: 13px 15px 13px;
    color:  black;
    text-decoration: underline;
}

.custom-top-menu .navbar-inner {
  @include border-radius(25px);
  height: 46px;

  .brand {
    display: block;
    width: 43px;
    height: 42px;
    background-image: url(<%= asset_path('go-home.png') %>);
    padding: 0;
    margin-top: 2px;
    margin-left: -18px;
  }

      .nav {
          li {
              a {
                  padding: 13px 25px 13px;
              }
          }
      }
}